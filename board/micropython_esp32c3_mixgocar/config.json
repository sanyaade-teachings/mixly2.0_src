{
    "board": {
        "MixGo Car 4.2": "micropython:esp32c3:mixgocar_c3"
       
    },
    "language": "MicroPython",
    "burn": {
        "type": "command",
        "portSelect": "all",
        "command": "\"{esptool}\" --chip esp32c3 --port {com} --baud 460800 erase_flash && \"{esptool}\" --chip esp32c3 --port {com} --baud 460800 write_flash 0x0 \"{indexPath}/build/esp32c3.bin\""
    },
    "upload": {
        "type": "command",
        "portSelect": "all",
        "command": "\"{ampy}\" -p {com} -d 1 put \"{indexPath}/build/upload\"",
        "filePath": "{indexPath}/build/upload/main.py",
        "copyLib": true,
        "reset": [
            {
                "dtr": false,
                "rts": true
            },{
                "sleep": 100
            },{
                "dtr": false,
                "rts": false
            },{
                "sleep": 100
            }
        ]
    },
    "nav": {
        "burn": true,
        "upload": true,
        "save": {
            "img": true,
            "py": true
        },
        "setting": {
            "thirdPartyLibrary": true
        }
    },
    "serial": {
        "ctrlCBtn": true,
        "ctrlDBtn": true,
        "baudRates": 115200,
        "yMax": 100,
        "yMin": 0,
        "pointNum": 100,
        "rts": true,
        "dtr": false
    },
    "lib": {
        "mixly": {
            "url": [
                "http://mixlylibs.cloud:8099/mixly2/cloud-libraries/raw/master/cloud-libs/micropython_esp32_mixgo/libs.json"
            ]
        }
    },
    "pythonToBlockly": false,
    "web": {
        "com": "serial",
        "burn": {
            "erase": true,
            "binFile": [
                {
                    "offset": "0x1000",
                    "path": "./build/MixGo_Car-0x0-V1.19.1-lib.bin"
                }, {
                    "offset": "0x3A0000",
                    "path": "../micropython_common/build/HZK12.bin"
                }
            ]
        },
        "upload": {
            "reset": []
        }
    }
}