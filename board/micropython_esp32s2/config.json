{
    "board": {
        "MixGo CE": "micropython:esp32s2:mixgo_ce"
    },
    "language": "MicroPython",
    "burn": {
        "type": "command",
        "portSelect": "all",
        "micropython:esp32s2:mixgo_ce": {
            "command": "\"{esptool}\" --chip esp32s2 --port {com} --baud 460800 --after=no_reset_stub erase_flash && \"{esptool}\" --chip esp32s2 --port {com} --baud 460800 --after=no_reset write_flash 0x1000 \"{indexPath}/build/MixGo_CE-0x1000-V1.19.1-220901.bin\" 0X3A0000 \"{indexPath}/build/HZK12.bin\""
        },        
        "micropython:esp32s2:generic": {
            "command": "\"{esptool}\" --chip esp32s2 --port {com} --baud 460800 --after=no_reset_stub erase_flash && \"{esptool}\" --chip esp32s2 --port {com} --baud 460800 --after=no_reset write_flash 0x1000 \"{indexPath}/build/MixGo_CE-0x1000-V1.19.1-220901.bin\""
        },
        "special": [
            {
                "name": "ESP-AT-mode",
                "command": "\"{esptool}\" --chip esp32s2 --port {com} --baud 460800 --after=no_reset_stub erase_flash && \"{esptool}\" --chip esp32s2 --port {com} --baud 460800 --after=no_reset write_flash 0x0000 \"{indexPath}/build/MixGo-CE_AT-T17_R18.bin\""
            }
        ]
    },
    "upload": {
        "type": "command",
        "portSelect": "all",
        "libPath": [
            "{indexPath}/build/lib",
            "{indexPath}/../micropython_common/build/lib"
        ],
        "command": "\"{ampy}\" -p {com} -d 1 -r \"{reset}\" put \"{indexPath}/build/upload\"",
        "filePath": "{indexPath}/build/upload/main.py",
        "copyLib": true,
        "reset": []
    },
    "nav": {
        "burn": true,
        "upload": true,
        "save": {
            "img": true,
            "py": true
        },
        "setting": {
            "thirdPartyLibrary": true
        }
    },
    "serial": {
        "ctrlCBtn": true,
        "ctrlDBtn": true,
        "baudRates": 115200,
        "yMax": 100,
        "yMin": 0,
        "pointNum": 100,
        "rts": true,
        "dtr": true
    },
    "lib": {
        "mixly": {
            "url": [
                "http://114.132.216.192:8099/mixly2/cloud-libraries/raw/master/cloud-libs/micropython_esp32c3/libs.json"
            ]
        }
    },
    "pythonToBlockly": false,
    "web": {
        "com": "serial",
        "burn": {
            "erase": true,
            "micropython:esp32s2:mixgo_ce": {
                "binFile": [
                    {
                        "offset": "0x1000",
                        "path": "./build/MixGo_CE-0x1000-V1.19.1-220902-lib.bin"
                    }, {
                        "offset": "0x3A0000",
                        "path": "./build/HZK12.bin"
                    }
                ]
            },
            "micropython:esp32s2:generic": {
                "binFile": [
                    {
                        "offset": "0x1000",
                        "path": "./build/MixGo_CE-0x1000-V1.19.1-220902-lib.bin"
                    }
                ]
            }
        
        }
    }
}