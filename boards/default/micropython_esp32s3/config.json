{
    "board": {
        "MixGo Zi": "micropython:esp32s3:yuankong_zi"
    },
    "language": "MicroPython",
    "burn": {
        "type": "command",
        "portSelect": "all",
        "micropython:esp32s3:yuankong_zi": {
            "command": "\"{esptool}\" --chip esp32s3 --port {com} --baud 460800 --before default_reset --after=no_reset_stub erase_flash && \"{esptool}\" --chip esp32s3 --port {com} --baud 460800 --after hard_reset write_flash 0x0 \"{indexPath}/build/YuanKong_ZI_S3-v1.21.0.bin\" 0x700000 \"{indexPath}/../micropython_common/build/HZK12.bin\""
        },
        "micropython:esp32s3:generic": {
            "command": "\"{esptool}\" --chip esp32s3 --port {com} --baud 460800 --after=no_reset_stub erase_flash && \"{esptool}\" --chip esp32s3 --port {com} --baud 460800 --after=no_reset write_flash 0x0 \"{indexPath}/build/YuanKong_ZI_S3-v1.21.0.bin\""
        },
        "special": [
            {
                "name": "ESP-AT-mode",
                "command": "\"{esptool}\" --chip esp32s3 --port {com} --baud 460800 --after=no_reset_stub erase_flash && \"{esptool}\" --chip esp32s3 --port {com} --baud 460800 --after=no_reset write_flash 0x0000 \"{indexPath}/build/MixGo-CE_AT-T17_R18.bin\""
            }
        ]
    },
    "upload": {
        "type": "command",
        "portSelect": "all",
        "libPath": [
            "{indexPath}/build/lib",
            "{indexPath}/../micropython_common/build/lib"
        ],
        "command": "\"{ampy}\" -p {com} -d 1 -r \"{reset}\" put \"{indexPath}/build/upload\"",
        "filePath": "{indexPath}/build/upload/main.py",
        "copyLib": true,
        "reset": []
    },
    "nav": {
        "burn": true,
        "upload": true,
        "save": {
            "py": true
        },
        "setting": {
            "thirdPartyLibrary": true
        }
    },
    "serial": {
        "ctrlCBtn": true,
        "ctrlDBtn": true,
        "baudRates": 115200,
        "yMax": 100,
        "yMin": 0,
        "pointNum": 100,
        "rts": true,
        "dtr": true
    },
    "lib": {
        "mixly": {
            "url": [
                "http://mixlylibs.cloud:8099/mixly2/cloud-libraries/raw/develop/cloud-libs/micropython_esp32c3/libs.json"
            ]
        }
    },
    "pythonToBlockly": false,
    "web": {
        "com": "serial",
        "burn": {
            "erase": true,
            "micropython:esp32s3:yuankong_zi": {
                "binFile": [
                    {
                        "offset": "0x0000",
                        "path": "./build/YuanKong_ZI_S3-v1.21.0.bin"
                    }, {
                        "offset": "0x3A0000",
                        "path": "../micropython_common/build/HZK12.bin"
                    }
                ]
            },
            "micropython:esp32s3:generic": {
                "binFile": [
                    {
                        "offset": "0x0000",
                        "path": "./build/YuanKong_ZI_S3-v1.21.0.bin"
                    }
                ]
            }
        },
        "upload": {
            "reset": []
        }
    }
}
