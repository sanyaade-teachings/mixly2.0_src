{
    "board": {
        "MixGo CC": "micropython:esp32c3:mixgo_cc",
        "MixGo ME": "micropython:esp32c3:mixgo_me",
        "ESP32C3 Generic": "micropython:esp32c3:generic"
    },
    "language": "MicroPython",
    "burn": {
        "portSelect": "all",
        "micropython:esp32c3:mixgo_cc": {
            "command": "\"{esptool}\" --chip esp32c3 --port {com} --baud 460800 erase_flash && \"{esptool}\" --chip esp32c3 --port {com} --baud 460800 write_flash 0x0 \"{indexPath}/build/MixGo_CC-0x0-V1.19.1-220701.bin\" 0X3A0000 \"{indexPath}/build/HZK12.bin\""
        },
        "micropython:esp32c3:mixgo_me": {
            "command": "\"{esptool}\" --chip esp32c3 --port {com} --baud 460800 erase_flash && \"{esptool}\" --chip esp32c3 --port {com} --baud 460800 write_flash 0x0 \"{indexPath}/build/MixGo_ME-0x0-V1.19.1-220701.bin\""
        },
        "micropython:esp32c3:generic": {
            "command": "\"{esptool}\" --chip esp32c3 --port {com} --baud 460800 erase_flash && \"{esptool}\" --chip esp32c3 --port {com} --baud 460800 write_flash 0x0 \"{indexPath}/build/MixGo_CC-0x0-V1.19.1-220701.bin\""
        }
    },
    "upload": {
        "type": "ampy",
        "portSelect": "all",
        "libPath": [
            "{indexPath}/build/lib",
            "{indexPath}/../micropython_common/build/lib"
        ],
        "command": "\"{ampy}\" -p {com} -d 1 -r \"{reset}\" put \"{indexPath}/build/upload\"",
        "filePath": "{indexPath}/build/upload/main.py",
        "copyLib": true,
        "reset": [
            {
                "dtr": false,
                "rts": true
            }, {
                "sleep": 300
            }, {
                "dtr": false,
                "rts": false
            }, {
                "sleep": 300
            }, {
                "dtr": false,
                "rts": true
            }, {
                "sleep": 300
            }
        ]
    },
    "nav": {
        "burn": true,
        "upload": true,
        "save": {
            "img": true,
            "py": true
        },
        "setting": {
            "thirdPartyLibrary": true
        }
    },
    "serial": {
        "ctrlCBtn": true,
        "ctrlDBtn": true,
        "baudRates": 115200,
        "yMax": 100,
        "yMin": 0,
        "pointNum": 100,
        "rts": true,
        "dtr": false
    },
    "lib": {
        "mixly": {
            "url": [
                "http://114.132.216.192:8099/mixly2/cloud-libraries/raw/master/cloud-libs/micropython_esp32c3/libs.json"
            ]
        }
    },
    "pythonToBlockly": false,
    "web": {
        "burn": {
            "erase": true,
            "micropython:esp32c3:mixgo_cc": {
                "binFile": [
                    {
                        "offset": "0x0000",
                        "path": "./build/MixGo_CC-0x0-V1.19.1-220701.bin"
                    }, {
                        "offset": "0x3A0000",
                        "path": "./build/HZK12.bin"
                    }
                ]
            },
            "micropython:esp32c3:mixgo_me": {
                "binFile": [
                    {
                        "offset": "0x0000",
                        "path": "./build/MixGo_ME-0x0-V1.19.1-220722-lib.bin"
                    }
                ]
            },
            "micropython:esp32c3:generic": {
                "binFile": [
                    {
                        "offset": "0x0000",
                        "path": "./build/MixGo_CC-0x0-V1.19.1-220701.bin"
                    }
                ]
            }
        },
        "esp32c3": {
