	"description": "Mixly",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"buildWin32Ia32": "electron-packager ./ Mixly --platform=win32 --arch=ia32  --out=./app --app-version=2.0.0 --overwrite --icon=./files/mixly.ico",
		"buildWin32X64": "electron-packager ./ Mixly --platform=win32 --arch=x64  --out=./app --app-version=2.0.0 --overwrite --icon=./files/mixly.ico",
		"buildDarwinX64": "electron-packager ./ Mixly --platform=darwin  --out=./app --app-version=2.0.0 --overwrite --icon=./files/mixly.icns",
		"buildLinuxX64": "electron-packager ./ Mixly --platform=linux --arch=x64 --out=./app --app-version=2.0.0 --overwrite --icon=./files/mixly.png",
		"buildLinuxARM64": "electron-packager ./ Mixly --platform=linux --arch=arm64 --out=./app --app-version=2.0.0 --overwrite --icon=./files/mixly.png"
	},
	"author": "Mixly",
